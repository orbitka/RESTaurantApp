<h1>Log in:</h1>

<%= form_tag '/sessions', method: :post do %>
<%= label_tag :username %>
<%= text_field_tag :username %>

<%= label_tag :password %>
<%= password_field_tag :password %>

<%= submit_tag "Log In" %>
<% end %>

